[flake8]
ignore = C101,D403,F403,I100,I201,W503,W504
exclude = =
        .git,
        __pycache__,
        venv
filename = *.py
accept-encodings=utf-8
inline-qoutes = single
max-complexity = 12
max-linenumber = 500
max-line-legnth = 119
multiline-quotes = double
per-files-ignore = 
        core/project/settings/templates/settings.dev.py:F821